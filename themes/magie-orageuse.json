{
    "name": "Magie Orageuse",
    "type": "dark",
    "$schema": "vscode://schemas/color-theme",
    "colors": {
        "editor.background": "#24202e",
        "editor.foreground": "#ccd0dd",
        "activityBarBadge.background": "#05798d",
        "sideBarTitle.foreground": "#bbbbbb",
        "activityBar.background": "#272233",
        "tab.inactiveBackground": "#33303d",
        "tab.inactiveForeground": "#909fa8",
        "tab.activeBackground": "#333357",
        "tab.activeForeground": "#bcb6d8"
    },
    "tokenColors": [
        {
            "name": "Commentaires",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#55525c"
            }
        },
        {
            "name": "Invalides",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#FF5370"
            }
        },
        {
            "name": "Opérateurs",
            "scope": [
                "keyword.operator",
                "constant.other.color",
                "punctuation"
            ],
            "settings": {
                "foreground": "#b1b5d8"
            }
        },
        {
            "name": "Mot-clés",
            "scope": [
                "keyword",
                "keyword.control",
                "storage"
            ],
            "settings": {
                "foreground": "#7cb3ab",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#73c3db",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Constantes",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#4a89ad",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Champs",
            "scope": [
                "variable.other"
            ],
            "settings": {
                "foreground": "#bea7d3",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#f7f1fd"
            }
        },
        {
            "name": "Imports",
            "scope": [
                "variable.parameter.import"
            ],
            "settings": {
                "foreground": "#749e7e"
            }
        },
        {
            "name": "Fonctions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call",
                "variable.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#a2aed1",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#96b5ca",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#dd4545"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Nombres, Lettres",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#cf9d9d"
            }
        },
        {
            "name": "Textes",
            "scope": "string",
            "settings": {
                "foreground": "#bcc99a"
            }
        },
        {
            "name": "Textes: Caractères d’Échappement",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#8b8f81"
            }
        },
        {
            "name": "Textes: Expressions Régulières",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#3c6294"
            }
        }
    ]
}