{
    "name": "Magie Lumineuse",
    "type": "light",
    "$schema": "vscode://schemas/color-theme",
    "colors": {
        "editor.background": "#fafafa",
        "editor.foreground": "#333333",
        "list.activeSelectionIconForeground": "#fff",
        "tab.inactiveBackground": "#d0dada",
        "tab.inactiveForeground": "#4a494b",
        "tab.activeBackground": "#d7ecb4",
        "tab.activeForeground": "#2e3622"
    },
    "tokenColors": [
        {
            "name": "Commentaires",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#848299"
            }
        },
        {
            "name": "Invalides",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#FF5370"
            }
        },
        {
            "name": "Opérateurs",
            "scope": [
                "keyword.operator",
                "constant.other.color",
                "punctuation"
            ],
            "settings": {
                "foreground": "#8a3a7d"
            }
        },
        {
            "name": "Mot-clés",
            "scope": [
                "keyword",
                "keyword.control",
                "storage"
            ],
            "settings": {
                "foreground": "#00a71a"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#634bd7"
            }
        },
        {
            "name": "Constantes",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#634bd7"
            }
        },
        {
            "name": "Champs",
            "scope": [
                "variable.other"
            ],
            "settings": {
                "foreground": "#6c7b23"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Imports",
            "scope": [
                "variable.parameter.import"
            ],
            "settings": {
                "foreground": "#d98922"
            }
        },
        {
            "name": "Fonctions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call",
                "variable.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#7d480f"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#634bd7"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#dd4545"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Nombres, Lettres",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#f500ac"
            }
        },
        {
            "name": "Textes",
            "scope": "string",
            "settings": {
                "foreground": "#b52dd1"
            }
        },
        {
            "name": "Textes: Caractères d’Échappement",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "Textes: Expressions Régulières",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#4B83CD"
            }
        }
    ]
}