{
    "name": "Magie Obscure",
    "type": "dark",
    "$schema": "vscode://schemas/color-theme",
    "colors": {
        "editor.background": "#23202b",
        "editor.foreground": "#ecdff5",
        "activityBarBadge.background": "#cc008f",
        "sideBarTitle.foreground": "#bbbbbb",
        "activityBar.background": "#272233",
        "tab.inactiveBackground": "#8b7d62",
        "tab.inactiveForeground": "#2c251b",
        "tab.activeBackground": "#bcdd87",
        "tab.activeForeground": "#2e3622"
    },
    "tokenColors": [
        {
            "name": "Commentaires",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#546E7A"
            }
        },
        {
            "name": "Invalides",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#FF5370"
            }
        },
        {
            "name": "Opérateurs",
            "scope": [
                "keyword.operator",
                "constant.other.color",
                "punctuation"
            ],
            "settings": {
                "foreground": "#b1b5d8"
            }
        },
        {
            "name": "Mot-clés",
            "scope": [
                "keyword",
                "keyword.control",
                "storage"
            ],
            "settings": {
                "foreground": "#73db92"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#a1a3ff"
            }
        },
        {
            "name": "Constantes",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#e7de8e"
            }
        },
        {
            "name": "Champs",
            "scope": [
                "variable.other"
            ],
            "settings": {
                "foreground": "#ffe6c1"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#f1fdfd"
            }
        },
        {
            "name": "Imports",
            "scope": [
                "variable.parameter.import"
            ],
            "settings": {
                "foreground": "#ffd597"
            }
        },
        {
            "name": "Fonctions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call",
                "variable.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#c3e9be"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#7ed0e9"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#dd4545"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Nombres, Lettres",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#d39ff5"
            }
        },
        {
            "name": "Textes",
            "scope": "string",
            "settings": {
                "foreground": "#ecad90"
            }
        },
        {
            "name": "Textes: Caractères d’Échappement",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "Textes: Expressions Régulières",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#4B83CD"
            }
        }
    ]
}