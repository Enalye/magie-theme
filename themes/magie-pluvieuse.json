{
    "name": "Magie Pluvieuse",
    "type": "light",
    "$schema": "vscode://schemas/color-theme",
    "colors": {
        "editor.background": "#d0d7d8",
        "editor.foreground": "#79747a",
        "list.activeSelectionIconForeground": "#fff",
        "tab.inactiveBackground": "#d1d3ce",
        "tab.inactiveForeground": "#2e3622",
        "tab.activeBackground": "#b2bdbd",
        "tab.activeForeground": "#4a494b"
    },
    "tokenColors": [
        {
            "name": "Commentaires",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#848299"
            }
        },
        {
            "name": "Invalides",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#FF5370"
            }
        },
        {
            "name": "Opérateurs",
            "scope": [
                "keyword.operator",
                "constant.other.color",
                "punctuation"
            ],
            "settings": {
                "foreground": "#5b6669"
            }
        },
        {
            "name": "Mot-clés",
            "scope": [
                "keyword",
                "keyword.control",
                "storage"
            ],
            "settings": {
                "foreground": "#1b8375",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#8262a7",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Constantes",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#927aa7",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Champs",
            "scope": [
                "variable.other"
            ],
            "settings": {
                "foreground": "#5f8d96",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Imports",
            "scope": [
                "variable.parameter.import"
            ],
            "settings": {
                "foreground": "#70547e"
            }
        },
        {
            "name": "Fonctions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call",
                "variable.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#5a6372",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#4f8d8f",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#dd4545"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Nombres, Lettres",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#3a8d53"
            }
        },
        {
            "name": "Textes",
            "scope": "string",
            "settings": {
                "foreground": "#679bb9"
            }
        },
        {
            "name": "Textes: Caractères d’Échappement",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#4f3c96"
            }
        },
        {
            "name": "Textes: Expressions Régulières",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#509480"
            }
        }
    ]
}